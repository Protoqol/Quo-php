{
  "name": "protoqol/quo",
  "version": "0.0.2",
  "description": "Composer package for Quo client.",
  "authors": [
    {
      "name": "Quinten Justus",
      "email": "quinten@protoqol.nl"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "symfony/polyfill-mbstring": "~1.0",
    "symfony/polyfill-php80": "^1.16",
    "guzzlehttp/guzzle": "^7.4",
    "ext-json": "*",
    "ext-iconv": "*"
  },
  "autoload": {
    "files": [
      "src/Helpers/functions/quo.php",
      "src/Helpers/functions/polyfill.php"
    ],
    "psr-4": {
