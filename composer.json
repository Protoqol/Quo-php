{
  "name": "protoqol/quo-php",
  "version": "0.2.0",
  "description": "PHP - Quo package.",
  "authors": [
    {
      "name": "Quinten Justus",
      "email": "quinten@protoqol.nl"
    }
  ],
  "require": {
    "php": ">=7.1|>8.0",
    "ext-json": "*",
    "ext-iconv": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "9"
  },
  "provide": {
    "ext-mbstring": "*"
  },
  "suggest": {
    "ext-mbstring": "For the best VarDumper performance."
  },
  "bin": [
    "bin/quo"
  ],
  "autoload": {
    "files": [
      "src/functions/quo.php",
      "src/Polyfill/polyfill.php",
      "src/Polyfill/Mbstring/bootstrap.php"
    ],
    "psr-4": {
      "Protoqol\\Quo\\": "src/"
    }
  },
  "extra": {
    "thanks": {
      "name": "symfony/var-dumper",
      "url": "https://github.com/symfony/var-dumper"
    }
  }
}
