{
  "name": "protoqol/quo-php",
  "version": "0.2.0",
  "description": "PHP - Quo package.",
  "authors": [
    {
      "name": "Quinten Justus",
      "email": "quinten@protoqol.nl"
    }
  ],
  "require": {
    "php": ">=7.1|>8.0",
    "ext-json": "*",
    "ext-iconv": "*",
    "ext-curl": "*"
  },
  "provide": {
    "ext-mbstring": "*"
  },
  "suggest": {
    "ext-mbstring": "For best VarDumper performance."
  },
  "autoload": {
    "files": [
      "src/Helpers/functions/quo.php",
      "src/Polyfill/polyfill.php",
      "src/Polyfill/Mbstring/bootstrap.php"
    ],
    "psr-4": {
      "Protoqol\\Quo\\": "src/"
    }
  }
}
